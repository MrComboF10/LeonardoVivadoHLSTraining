#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"

float gravity(float m1, float m2, float d) {
	return 10.0f * (m1 * m2) / (d * d);
}


int main()
{
    init_platform();

    print("Hello World\n\r");
    print("Successfully ran Hello World application");
    cleanup_platform();
    return 0;
}
